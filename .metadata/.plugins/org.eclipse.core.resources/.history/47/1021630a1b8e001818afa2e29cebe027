package com.internousdev.login.dto;

public class loginDTO {
	private int id;
	private String name;
	private String password;

	public int getId(){
	return id;
	}

	public void setId(int id)
	this.id = id;

	getName(String name)
	return this.name;

	setName(String name)
	this.name = name;

	getPassword(String password)
	return this.password;

	setPassword(String password)
	this.password = password;
}
