package com.internousdev.login.dto;

public class loginDTO {
	private int_id;
	private String name;
	private String password;

	getId(int_id)
	return this.id;

	setId(int_id)
	this.id = id;

	getName(String name)
	return this.name;

	setName(String name)
	this.name = name;

	getPassword(String password)
	return this.password;

	setPassword(String password)
	this.password = password;
}
