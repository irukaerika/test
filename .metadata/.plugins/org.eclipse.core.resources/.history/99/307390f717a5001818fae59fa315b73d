<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.ecsite3.action" extends="struts-default">

		<!-- ItemDetailsAction -->
		<action name="ItemDetailsAction"
		class="com.internousdev.ecsite3.action.ItemDetailsAction" method="execute">
			<result name="success">itemDetails.jsp</result>
		</action>

		<!-- GoAddItemAction -->
		<action name="GoAddItemAction"
		class="com.internousdev.ecsite3.action.GoAddItemAction" method="execute">
			<result name="success">addItem.jsp</result>
		</action>

		<!-- AddItemConfirmAction -->
		<action name="AddItemConfirmAction"
		class="com.internousdev.ecsite3.action.AddItemConfirmAction" method="execute">
			<result name="success">addItemConfirm.jsp</result>
			<result name="error">addItem.jsp</result>
		</action>

		<!-- AddItemCompleteAction -->
		<action name="AddItemCompleteAction"
		class="com.internousdev.ecsite3.action.AddItemCompleteAction" method="execute">
			<result name="success">addItemComplete.jsp</result>
		</action>

		<!-- AddItemListAction -->
		<action name="AddItemListAction"
		class="com.internousdev.ecsite3.action.AddItemListAction" method="execute">
			<result name="success">addItemList.jsp</result>
		</action>

		<!-- UserListAction -->
		<action name="UserListAction"
		class="com.internousdev.ecsite3.action.UserListAction" method="execute">
			<result name="success">userList.jsp</result>
		</action>

		<!-- GoHomeAction -->
		<action name="GoHomeAction"
		class="com.internousdev.ecsite3.action.GoHomeAction" method="execute">
			<result name="success">home.jsp</result>
		</action>

		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.ecsite3.action.HomeAction"
		method="execute">
			<result name="login">login.jsp</result>
			<result name="success">buyItem.jsp</result>
		</action>

		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.ecsite3.action.LoginAction"
		method="execute">
			<result name="success">admin.jsp</result>
			<result name="login">buyItem.jsp</result>
			<result name="error">login.jsp</result>
		</action>

		<!-- UserCreateAction -->
		<action name="UserCreateAction" class="com.internousdev.ecsite3.action.UserCreateAction"
		method="execute">
			<result name="success">userCreate.jsp</result>
			<result name="back">login.jsp</result>
		</action>

		<!-- UserCreateConfirmAction -->
		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite3.action.UserCreateConfirmAction"
		method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<!-- UserCreateCompleteAction -->
		<action name="UserCreateCompleteAction"
		class="com.internousdev.ecsite3.action.UserCreateCompleteAction"
		method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>

		<!-- BuyItemAction -->
		<action name="BuyItemAction" class="com.internousdev.ecsite3.action.BuyItemAction"
		method="execute">
			<result name="success">buyItemConfirm.jsp</result>
			<result name="error">buyItem.jsp</result>
		</action>

		<!-- BuyItemConfirmAction -->
		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite3.action.BuyItemConfirmAction"
		method="execute">
			<result name="success">buyItem.jsp</result>
		</action>

		<!-- MyPageAction -->
		<action name="MyPageAction" class="com.internousdev.ecsite3.action.MyPageAction" method="execute">
			<result name="success">myPage.jsp</result>
		</action>

		<!-- LogoutAction -->
		<action name="LogoutAction" class="com.internousdev.ecsite3.action.LogoutAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>
	</package>
