<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.ecsite2.action" extends="struts-default">

		<!-- GoEditItemAction -->
		<action name="GoEditItemAction" class="com.internousdev.ecsite2.action.GoEditItemAction" method="execute">
			<result name="success">updateItem.jsp</result>
		</action>

		<!-- UpdateItemAction -->
		<action name="UpdateItemAction" class="com.internousdev.ecsite2.action.UpdateItemAction" method="execute">
			<result name="success">updateItem.jsp</result>
		</action>

		<!-- GoEditUserAction -->
		<action name="GoEditUserAction" class="com.internousdev.ecsite2.action.GoEditUserAction" method="execute">
			<result name="success">updateUser.jsp</result>
		</action>

		<!-- UpdateUserAction -->
		<action name="UpdateUserAction" class="com.internousdev.ecsite2.action.UpdateUserAction" method="execute">
			<result name="success">updateUser.jsp</result>
		</action>

		<!-- GoLoginAction -->
		<action name="GoLoginAction" class="com.internousdev.ecsite2.action.GoLoginAction" method="execute">
			<result name="success">login.jsp</result>
		</action>

		<!-- AdminAction -->
		<action name="AdminAction"
		class="com.internousdev.ecsite2.action.AdminAction" method="execute">
			<result name="success">admin.jsp</result>
		</action>

		<!-- UserDetailsAction -->
		<action name="UserDetailsAction"
		class="com.internousdev.ecsite2.action.UserDetailsAction" method="execute">
			<result name="success">userDetails.jsp</result>
		</action>

		<!--DeleteUserDetailsAction -->
		<action name="DeleteUserDetailsAction"
		class="com.internousdev.ecsite2.action.DeleteUserDetailsAction" method="execute">
			<result name="success">userDetails.jsp</result>
		</action>

		<!-- DeleteItemAction -->
		<action name="DeleteItemAction"
		class="com.internousdev.ecsite2.action.DeleteItemAction" method="execute">
			<result name="success">itemDetails.jsp</result>
		</action>

		<!-- ItemDetailsAction -->
		<action name="ItemDetailsAction"
		class="com.internousdev.ecsite2.action.ItemDetailsAction" method="execute">
			<result name="success">itemDetails.jsp</result>
		</action>

		<!-- GoAddItemAction -->
		<action name="GoAddItemAction"
		class="com.internousdev.ecsite2.action.GoAddItemAction" method="execute">
			<result name="success">addItem.jsp</result>
		</action>

		<!-- AddItemConfirmAction -->
		<action name="AddItemConfirmAction"
		class="com.internousdev.ecsite2.action.AddItemConfirmAction" method="execute">
			<result name="success">addItemConfirm.jsp</result>
			<result name="error">addItem.jsp</result>
		</action>

		 <!-- AddItemCompleteAction -->
		<action name="AddItemCompleteAction"
		class="com.internousdev.ecsite2.action.AddItemCompleteAction" method="execute">
			<result name="success">addItemComplete.jsp</result>
		</action>

		<!-- AddItemListAction -->
		<action name="AddItemListAction"
		class="com.internousdev.ecsite2.action.AddItemListAction" method="execute">
			<result name="success">addItemList.jsp</result>
		</action>

		<!-- UserListAction -->
		<action name="UserListAction"
		class="com.internousdev.ecsite2.action.UserListAction" method="execute">
			<result name="success">userList.jsp</result>
		</action>

		<!-- GoHomeAction -->
		<action name="GoHomeAction"
	class="com.internousdev.ecsite2.action.GoHomeAction" method="execute">
			<result name="success">home.jsp</result>
		</action>

		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.ecsite2.action.HomeAction" method="execute">
			<result name="login">login.jsp</result>
			<result name="success">buyItem.jsp</result>
		</action>

		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.ecsite2.action.LoginAction" method="execute">
			<result name="success">admin.jsp</result>
			<result name="login">buyItem.jsp</result>
			<result name="error">login.jsp</result>
		</action>

		<!-- UserCreateAction -->
		<action name="UserCreateAction" class="com.internousdev.ecsite2.action.UserCreateAction" method="execute">
			<result name="success">userCreate.jsp</result>
			<result name="back">login.jsp</result>
		</action>

		<!-- UserCreateConfirmAction -->
		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite2.action.UserCreateConfirmAction" method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
       	</action>

		<!-- UserCreateCompleteAction -->
		<action name="UserCreateCompleteAction"
class="com.internousdev.ecsite2.action.UserCreateCompleteAction" method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>

		<!-- BuyItemAction -->
		<action name="BuyItemAction" class="com.internousdev.ecsite2.action.BuyItemAction" method="execute">
			<result name="success">buyItemConfirm.jsp</result>
			<result name="error">buyItem.jsp</result>
		</action>

		<!-- BuyItemConfirmAction -->
		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite2.action.BuyItemConfirmAction" method="execute">
			<result name="success">buyItemComplete.jsp</result>
			<result name="back">buyItem.jsp</result>
		</action>

   		<!-- MyPageAction -->
   		<action name="MyPageAction" class="com.internousdev.ecsite2.action.MyPageAction" method="execute">
   			<result name="success">myPage.jsp</result>
   		</action>

   		<!-- LogouotAction -->
   		<action name="LogoutAction" class="com.internousdev.ecsite2.action.LogoutAction" method="execute">
   			<result name="success">home.jsp</result>
        </action>
	</package>
</struts>